#include<bits/stdc++.h>	
using namespace std;
void bina(int n){
	for(int i=1<<2;i>0;i=i/2)
		(n&i)?cout<<"1":cout<<"0";
}
int subsetSum(int a[],int s,int n){
	int count=0,sum=0,mask=0;
	while(mask < (1<<n)){
		sum=0;
		for(int i=0;i<n;i++){
			bina(mask); cout<<"\n";	bina(1<<i); cout<<" ";
			if(mask & (1<<i))	sum+=a[i],	cout<<sum;
			cout<<endl<<endl;
		}	
		if(sum==s)	count++,	cout<<"->"<<count<<endl;
		mask++;cout<<"-------\n";
	}
	return count;
}
int main(){
	freopen("input.txt","r",stdin);	//freopen("output.txt","w",stdout);
	int n;	cin>>n;
	int a[n];
	for(int i=0;i<n;i++)	cin>>a[i];
	int s;	cin>>s;
	cout<<subsetSum(a,s,n)<<endl;
    return 0;
}


#include<bits/stdc++.h>
using namespace std;
#define MAX 100
int M[MAX][MAX];
int maxAreaHisto(int a[],int n){
	stack<int> s;
	int ma=-1,sum,tp,i=0;
	while(i<n){
		if(s.empty()||a[s.top()]<=a[i])	s.push(i++);
		else{
			int x=s.top(); s.pop();
			sum=a[x]*(s.empty()?i:i-s.top()-1);
			ma=max(sum,ma);
		}
	}
	while(!s.empty()){
		int x=s.top(); s.pop();
		sum=a[x]*(s.empty()?i:i-s.top()-1);
		ma=max(sum,ma);
	}
	return ma;
}
int maxArea(int n,int m){
	int a[m],ans=-1; memset(a,0,sizeof(a));
	for(int i=0;i<n;i++){
		for(int j=0;j<m;j++) (M[i][j])?a[j]+=M[i][j]:a[j]=0;
		int xu=maxAreaHisto(a,m);
		ans=max(ans,xu);
	}
	return ans;
}
int main(){
	freopen("input.txt","r",stdin);
	int n,m; cin>>n>>m;
	//int a[n][m];
	for(int i=0;i<n;i++)for(int j=0;j<m;j++) cin>>M[i][j];
	cout<<maxArea(n,m);
	cout<<endl;
	return 0;
}
